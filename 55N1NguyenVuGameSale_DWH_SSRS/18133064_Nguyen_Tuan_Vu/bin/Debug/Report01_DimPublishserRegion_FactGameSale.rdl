      </ConnectionProperties>
      <rd:SecurityType>DataBase</rd:SecurityType>
      <rd:DataSourceID>98108ef8-23e6-436b-a908-c0e5cd1e7e81</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Dataset_Publisher">
      <Query>
        <DataSourceName>video_games_DW</DataSourceName>
        <CommandText>SELECT        Publisher
FROM            (SELECT DISTINCT TOP (100) DimPublisher.publisher_name AS Publisher, COUNT(DimYear.release_year) AS countYear
                          FROM            FactGameSale INNER JOIN
                                                    DimPublisher ON FactGameSale.publisher_id = DimPublisher.id INNER JOIN
                                                    DimYear ON FactGameSale.year_id = DimYear.id
                          GROUP BY DimPublisher.publisher_name
                          ORDER BY countYear DESC) AS R</CommandText>
      </Query>
      <Fields>
        <Field Name="Publisher">
          <DataField>Publisher</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Dataset_Region">
      <Query>
        <DataSourceName>video_games_DW</DataSourceName>
        <CommandText>SELECT DISTINCT region_name AS Region
FROM            DimRegion
ORDER BY Region</CommandText>
      </Query>
      <Fields>
        <Field Name="Region">
          <DataField>Region</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Dataset_Chart">
      <Query>
        <DataSourceName>video_games_DW</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Region">
            <Value>=Parameters!Region.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Publisher">
            <Value>=Parameters!Publisher.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        DimPublisher.publisher_name, FactGameSale.sales, DimRegion.region_name
FROM            DimPublisher INNER JOIN
                         FactGameSale ON DimPublisher.id = FactGameSale.publisher_id INNER JOIN
                         DimRegion ON FactGameSale.region_id = DimRegion.id
WHERE        (DimRegion.region_name IN (@Region)) AND (DimPublisher.publisher_name IN (@Publisher))</CommandText>
      </Query>
      <Fields>
        <Field Name="publisher_name">
          <DataField>publisher_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="sales">
          <DataField>sales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="region_name">
          <DataField>region_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
